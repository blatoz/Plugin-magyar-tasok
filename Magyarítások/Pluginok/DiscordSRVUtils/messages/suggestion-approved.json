{
  "embed": {
    "thumbnail": {
      "url": "[submitter.effectiveAvatarUrl]"
    },
    "color": "green",
    "fields": [
      {
        "name": "Szavazási eredmények",
        "value": ":white_check_mark: [suggestion.yesCount]\n:x: [suggestion.noCount]"
      },
      {
        "name": "Beküldő",
        "value": "[submitter.asMention]"
      },
      {
        "name": "Ötlet",
        "value": "[suggestion.text]"
      },
      {
        "name": "Elfogadta",
        "value": "[approver.asMention]"
      }
    ],
    "title": "Őtlet száma: [suggestion.number]"
  }
}